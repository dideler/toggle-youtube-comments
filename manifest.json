{
  "manifest_version": 2,
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_desc__",
  "version": "2.2.1",
  "default_locale": "en",

  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },

  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
      "js": ["script.js"],
      "css": ["youtube.css"],
      "run_at": "document_end"
    }
  ]
}
